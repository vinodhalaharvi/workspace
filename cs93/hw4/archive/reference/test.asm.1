.data
	inputString: .asciiz "input string\n"
.text

	# $a0, $a1 -> $v0
	multiply:
		add $s0, $zero, $zero
		add $t0, $zero, $zero

		slt $t3, $a0, $zero # is first number neg?
		#slt $t4, $a1, $zero # is second number neg?

		loop: 
			srav  $t1, $a1, $t0 # shift multiplicand to right
			andi  $t2, $t1, 1
			beq   $t2, 1 label 
			back:
				addi  $t0, $t0, 1
				slti  $t8, $t0, 32
				bne   $t8, $zero, loop	

				beq $t3, $zero, skip2
				sub $v0, $zero, $v0
				skip2:
					#jr $ra
					add $a0, $v0, $zero
					li $v0, 1
					syscall

					li $v0, 10 
					syscall
			label:
				sllv $t3, $a0, $t0
				add $v0, $v0, $t3
				j back
		jr $ra

	# $a0 -> $a0
	mul10:
		sll $t0, $a0, 3
		sll $t1, $a0, 1 
		add $t0, $t0, $t1
		add $a0, $t0, $zero
		li $v0, 1
		syscall
		jr $ra


	main:
		addi $a0, $zero, -11
		addi $a1, $zero, -11
		#jal mul10
		jal multiply
		li $v0, 10 
		syscall
