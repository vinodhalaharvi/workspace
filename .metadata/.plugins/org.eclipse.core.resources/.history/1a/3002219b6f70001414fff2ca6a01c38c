 * 
 */
package cscie97.asn3.squaredesk.provider;

// TODO: Auto-generated Javadoc
/**
 * The Class Facility.
 */
public class Facility {
	
	/**
	 * Instantiates a new facility.
	 *
	 * @param type the type
	 */
	public Facility(String type) {
		this.category = null; 
		this.type = type;
	}

	/**
	 * Instantiates a new facility.
	 *
	 * @param type the type
	 * @param category the category
	 */
	public Facility(String type, String category) {
		this.type = type;
		this.category = category;
	}
	
	/**
	 * Gets the type.
	 *
	 * @return the type
	 */
	public String getType() {
		return type;
	}
	
	/**
	 * Sets the type.
	 *
	 * @param type
	 *            the new type
	 */
	public void setType(String type) {
		this.type = type;
	}
	
	/**
	 * Gets the category.
	 *
	 * @return the category
	 */
	public String getCategory() {
		if (category == null){ 
			return ""; 
		} else {
			return category;	
		}
	}
	
	/**
	 * Sets the category.
	 *
	 * @param category
	 *            the new category
	 */
	public void setCategory(String category) {
		this.category = category;
	}
	
	/** The type. */
	private String type; 
	
	/** The category. */
	private String category; 
	
}
